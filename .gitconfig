[user]
	name = Engine
	email = enginechen07@gmail.com
[http]
	proxy = socks5://127.0.0.1:1080
[https]
	proxy = socks5://127.0.0.1:1080
[color]
	ui = true
[alias]
    st = status
    co = checkout
    di = diff
    dc = diff --cached
    aa = add --all
    b = branch
    lg = "log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit"
    unstage = "reset HEAD"
    last = "log -1"
[merge]
    tool = vimdiff
